<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>HostelManagement.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>HostelManagement.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>HostelManagement.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RoomBookingServlet</servlet-name>
  	<display-name>RoomBookingServlet</display-name>
  	<description></description>
  	<servlet-class>HostelManagement.RoomBookingServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FeePaymentServlet</servlet-name>
  	<display-name>FeePaymentServlet</display-name>
  	<description></description>
  	<servlet-class>HostelManagement.FeePaymentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RoomBookingServlet</servlet-name>
  	<url-pattern>/RoomBookingServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FeePaymentServlet</servlet-name>
  	<url-pattern>/FeePaymentServlet</url-pattern>
  </servlet-mapping>
</web-app>
